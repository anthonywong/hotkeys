<?xml version="1.0"?>
<CONFIG model="Memorex MX2500 Keyboard">

  <PrevTrack    keycode="144"/>
  <Stop         keycode="164"/>
  <NextTrack    keycode="153"/>
  <Eject        keycode="158"/>
  <Play         keycode="162"/>

  <VolUp        keycode="176" adj="2"/>
  <VolDown      keycode="174" adj="2"/>
  <Mute         keycode="160"/>

  <WebBrowser   keycode="178"/>
  <Email        keycode="236"/>
  <NewsReader   keycode="166"/>
  <Calculator   keycode="161"/>
  <MyDocuments  keycode="159"/>
  <MyComputer   keycode="237"/>
  <DOS          keycode="148"/>
