<?xml version="1.0"?>

<definition>

  <config model="Logitech Cordless Freedom Optical Keyboard">
  
    <Go           keycode="233"/>
    <ScreenSaver  keycode="223"/>
  
    <Calculator   keycode="147"/> <!-- Finance -->
    <MyComputer   keycode="148"/> <!-- My Sites -->
    <Communities  keycode="149"/>
    <Favorites    keycode="230"/>
  
    <PrevTrack    keycode="144"/>
    <Play         keycode="162"/>
    <Stop         keycode="164"/>
    <NextTrack    keycode="153"/>
    <VolUp        keycode="176" adj="1"/>
    <VolDown      keycode="174" adj="1"/>
    <Mute         keycode="160"/>
    <Record       keycode="237"/> <!-- Media -->
  
    <Idea         keycode="146"/> <!-- iTouch -->
    <Email        keycode="236"/>
    <Shopping     keycode="145"/>
    <Search       keycode="229"/>
    <WebBrowser   keycode="178"/> <!-- My Home -->
  
    <userdef keycode="234" command="gnome-terminal -e nethack">NetHack</userdef>
    <!-- "Back" -->
  
    <!-- Clicking the wheel is keycode 140, I map it using xmodmap instead -->
  </config>
  
  <contributor>
    <name>Joe Wreschnig</name>
    <email>piman@sacredchao.net</email>
  </contributor>

</definition>
