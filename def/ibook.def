<?xml version="1.0"?>

<definition>

  <config model="iBook Internal Keyboard">
  
    <VolUp      keycode="158"/>
    <VolDown    keycode="165"/>
    <Mute       keycode="166"/>
  
    <!-- Apple PowerBook power management daemon -->
    <userdef    keycode="222" command="/sbin/snooze">PMU sleep</userdef>  
  
  </config>

  <contributor>
    <name>Derrik Pates</name>
    <email>dpates@dsdk12.net</email>
  </contributor>

</definition>
