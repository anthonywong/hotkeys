<?xml version="1.0"?>

<definition>

  <config model="Silitek SK7100 Keyboard">
  
    <PrevTrack    keycode="144"/>
    <NextTrack    keycode="153"/>
    <Eject        keycode="158"/>
    <Play         keycode="162"/>
    <Stop         keycode="164"/>
    <Pause        keycode="146"/>
  
    <VolUp        keycode="176" adj="2"/>
    <VolDown      keycode="174" adj="2"/>
    <Mute         keycode="160"/>
  
    <WebBrowser   keycode="178"/>
  
    <!-- Some nice programs need to be bound to these keys, suggestion welcome -->
  
    <userdef keycode="165" command="gtcd">GNOME CD player</userdef>
    <userdef keycode="152" command="gtv">SMPEG GTK+ MPEG audio/video player</userdef>
    <userdef keycode="158" command="xterm -rv">Xterm</userdef>
    <userdef keycode="151" command="xterm -rv">Xterm</userdef>
    <userdef keycode="166" command="xterm -rv">Xterm</userdef>
  
  </config>
  
  <contributor>
    <name>to be added</name>
    <email>to be added</email>
  </contributor>

</definition>
